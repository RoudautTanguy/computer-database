<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>computer-database</display-name>
   <servlet> 
	   <servlet-name>DashBoard</servlet-name> 
	   <servlet-class>com.excilys.cdb.servlet.ServletDashboard</servlet-class> 
   </servlet> 
   <servlet> 
	   <servlet-name>AddComputer</servlet-name> 
	   <servlet-class>com.excilys.cdb.servlet.ServletAddComputer</servlet-class> 
   </servlet> 
   <servlet-mapping> 
	   <servlet-name>DashBoard</servlet-name> 
	   <url-pattern>/dashboard</url-pattern> 
   </servlet-mapping>
   <servlet-mapping> 
	   <servlet-name>AddComputer</servlet-name> 
	   <url-pattern>/addComputer</url-pattern> 
   </servlet-mapping>
</web-app>